{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.9",
        "maxLambdaSize": "300mb",
        "includeFiles": ["main.py", "requirements.txt"]
      }
    }
  ],
  "routes": [
    { "src": "(.*)", "dest": "main.py" }
  ]
}

