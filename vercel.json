{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.9",
        "maxLambdaSize": "500mb",
        "includeFiles": ["main.py"]
      }
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "main.py"
    }
  ]
}
